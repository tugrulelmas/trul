@using Trul.WebUI.Helper

<div id="partialModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">@Html.GetGlobalResource("DataToSave")</h3>
  </div>
  <div class="modal-body">
    <span id="partialModalBody"></span>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">@Html.GetGlobalResource("Close")</button>
  </div>
</div>